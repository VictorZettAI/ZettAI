# API Endpoints
VITE_API_URL=http://localhost:3000
VITE_ANALYTICS_ENDPOINT=/api/analytics

# Security
VITE_RATE_LIMIT_WINDOW_MS=900000
VITE_RATE_LIMIT_MAX_REQUESTS=100
VITE_MAX_FORM_SUBMISSIONS=5

# Analytics
VITE_ANALYTICS_BATCH_SIZE=10
VITE_ANALYTICS_FLUSH_INTERVAL=5000
VITE_GA_TRACKING_ID=your-ga-tracking-id

# Feature Flags
VITE_ENABLE_ANALYTICS=true
VITE_ENABLE_RATE_LIMITING=true
VITE_ENABLE_CHATBOT=false

# External Services
VITE_OPENAI_API_KEY=your-api-key-here
VITE_GOOGLE_ANALYTICS_ID=your-ga-id-here
VITE_STRIPE_PUBLIC_KEY=your-stripe-public-key
VITE_MAPBOX_TOKEN=your-mapbox-token

# Debug and Development
VITE_DEBUG_MODE=false
VITE_API_TIMEOUT=15000

# WebSocket
VITE_WEBSOCKET_URL=ws://localhost:3000

# Error Tracking
VITE_ENABLE_SENTRY=false

# Cache Configuration
VITE_CACHE_TTL=3600

# Deployment
VITE_ENVIRONMENT=development
VITE_APP_VERSION=1.0.0

# App Configuration
VITE_APP_NAME=ZettAI
VITE_APP_SHORT_NAME=ZettAI
VITE_APP_DESCRIPTION=Soluciones de IA innovadoras para empresas
VITE_APP_THEME_COLOR=#ffffff
VITE_APP_BACKGROUND_COLOR=#ffffff

# Contact Information
VITE_CONTACT_EMAIL=contacto@zettai.com
VITE_CONTACT_PHONE=+34 900 123 456
VITE_CONTACT_ADDRESS=Calle Tecnolog√≠a 123, 28001 Madrid

# Social Media
VITE_SOCIAL_TWITTER=https://twitter.com/zettai
VITE_SOCIAL_LINKEDIN=https://linkedin.com/company/zettai
VITE_SOCIAL_GITHUB=https://github.com/zettai

# Feature Flags
VITE_ENABLE_CHAT=true
VITE_ENABLE_DARK_MODE=true
